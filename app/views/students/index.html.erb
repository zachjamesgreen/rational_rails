<h2 class='center-heading'>List of All Graduated Students</h2>

<table id='students_table' class='model-list'>
  <tr id='header_row'>
    <th>
      Student Name
    </th>
    <th>
      Creation Date
    </th>
  </tr>
  <%@students.each do |student| %>
    <tr id='<%="student_data_row_#{student.id}"%>'>
      <td>
        <a class='td-selection-area' href=<%="/students/#{student.id}"%>><%=student.name%></a>
        </td>
      <td>
        <%=student.created_at%>
        </td>
      </tr>
    <tr class='last-row'>
      <td colspan='2'>
        <a id='<%="edit_student_#{student.id}_btn"%>' class='custom-button edit' href='<%="/students/#{student.id}/edit"%>'>Edit</a>
        </td>
      </tr>
    <%end%>
  </table>
